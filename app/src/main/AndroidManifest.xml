<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rifat.hellodoctor">

    <uses-permission android:name="android.permission.INTERNET" />

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality. 
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".AppController"
        android:allowBackup="true"
        android:icon="@drawable/hellodoctor"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme66">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".loginP"
            android:label="LogIn Patient" />
        <activity
            android:name=".DocPatSelection"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Selection"
            android:label="Home" />
        <activity
            android:name=".loginD"
            android:label="LogIn Doctor" />
        <activity
            android:name=".signupP"
            android:label="Sign Up Patient" />
        <activity
            android:name=".signupD"
            android:label="Sign Up Doctor" />
        <activity
            android:name=".Patient_Home"
            android:label="Patient Home" />
        <!--
             The API key for Google Maps-based APIs is defined as a string resource.
             (See the file "res/values/google_maps_api.xml").
             Note that the API key is linked to the encryption key used to sign the APK.
             You need a different API key for each encryption key, including the release key that is used to
             sign the APK for publishing.
             You can define the keys for the debug and release targets in src/debug/ and src/release/. 
        -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity
            android:name=".listing"
            android:label="Nearby Hospitals" />
        <activity
            android:name=".Doctor_Home"
            android:label="Doctor Home" />
        <activity
            android:name=".Show_tips_title"
            android:label="Tips Title" />
        <activity
            android:name=".Edit_Blog"
            android:label="Edit Blog" />
        <activity
            android:name=".Tips_Details"
            android:label="Blog" />
        <activity
            android:name=".Show_disease"
            android:label="Diseases" />
        <activity
            android:name=".Disease_details"
            android:label="Symtoms" />
        <activity android:name=".Catagory_List_Show" />
        <activity
            android:name=".Doctors_Show_List"
            android:label="Available Doctors" />
        <activity
            android:name=".Doctor_Profile_Appointment"
            android:label="Doctor" />
        <activity
            android:name=".Navi"
            android:label="Patient Home"
            android:theme="@style/AppTheme66.NoActionBar" />
        <activity
            android:name=".Doc_Manage_Schedule"
            android:label="Manage Schedule" />
        <activity
            android:name=".Pat_multiple_search"
            android:label="Patient Search" />
        <activity android:name=".Pat_Show_Bookings" />
        <activity android:name=".Doc_View_Pat"></activity>
    </application>

</manifest>